@model List<Piizzapan.EntityLayer.Concrete.SocialMedia>

<div class="widget-social-icons mt-sm-5 mt-4">
    <h5 class="widget-title">Sosyal Medya</h5>
    <ul class="icon-rounded">
        @foreach (var item in Model)
        {
            <li><a style="font-size:30px; color:gray" href="@item.Url" target="_blank"><span class="@item.Icon"></span></a></li>

        }

    </ul>
</div>